                var pessoa = sorteio[i];
        %>
		<tr>
			<td id="tabela">
        <%=pessoa.nome1%> 
			</td>
			<td id="tabela">
        tirou 
			</td>
			<td id="tabela">
        <%=pessoa.nome2%> <br>  
			</td>
		</tr>
        <%}%>
		</table>
		<a class="btn btn-primary" href="/index.html">HOME</a>
      </fieldset>
	</div>
  </body>
</html>
